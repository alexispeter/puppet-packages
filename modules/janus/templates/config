[general]

debug_level = 4				; Debug/logging level, valid values are 0-7
debug_timestamps = yes		; Whether to show a timestamp for each log line
debug_colors = no			; Whether colors should be disabled in the log


; Certificate and key to use for DTLS and/or HTTPS/WSS.
[certificates]
cert_pem = /usr/local/share/janus/cert.pem
cert_key = /usr/local/share/janus/cert.key

[plugins]
disable = libjanus_voicemail.so,libjanus_recordplay.so, libjanus_sip.so, libjanus_videoroom.so, libjanus_videocall.so, libjanus_audiobridge.so, libjanus_streaming.so, libjanus_echotest.so

[webserver]
ws_ssl = yes					; Whether to enable secure WebSockets
ws_secure_port = 8989		; WebSockets server secure port, if enabled

